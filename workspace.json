    "supremegaming-angular": {
      "projectType": "application",
      "root": "apps/supremegaming-angular",
      "sourceRoot": "apps/supremegaming-angular/src",
      "prefix": "supremegaming",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/supremegaming-angular",
            "index": "apps/supremegaming-angular/src/index.html",
            "main": "apps/supremegaming-angular/src/main.ts",
            "polyfills": "apps/supremegaming-angular/src/polyfills.ts",
            "tsConfig": "apps/supremegaming-angular/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/supremegaming-angular/src/favicon.ico", "apps/supremegaming-angular/src/assets"],
            "styles": ["apps/supremegaming-angular/src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": ["webfontloader"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/supremegaming-angular/src/environments/environment.ts",
                  "with": "apps/supremegaming-angular/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "supremegaming-angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "supremegaming-angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "supremegaming-angular:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/supremegaming-angular/tsconfig.app.json",
              "apps/supremegaming-angular/tsconfig.spec.json",
              "apps/supremegaming-angular/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/supremegaming-angular/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/supremegaming-angular"],
          "options": {
            "jestConfig": "apps/supremegaming-angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "supremegaming-angular-e2e": {
      "root": "apps/supremegaming-angular-e2e",
      "sourceRoot": "apps/supremegaming-angular-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/supremegaming-angular-e2e/cypress.json",
            "tsConfig": "apps/supremegaming-angular-e2e/tsconfig.e2e.json",
            "devServerTarget": "supremegaming-angular:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "supremegaming-angular:serve:production"
            }
          }
        },
        "lint": {
